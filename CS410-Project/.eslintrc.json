{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/prop-types": 0,
        "indent": [2, 2, {
            "SwitchCase": 1,
            "offsetTernaryExpressions": true
        }],
        "react/jsx-filename-extension": [0],
        "react/react-in-jsx-scope": "off"
    }
}
